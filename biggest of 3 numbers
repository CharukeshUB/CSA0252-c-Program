#include<stdio.h>
#include<conio.h>
void main()
{
  int a,b,c;
  printf("Enter the A B and C values");
  scanf("%d%d%d",&a,&b,&c);
  if(a>b && a>c)
    printf("%d is the biggest value",a);
  else if(b>c && b>a)
    printf("%d is the biggest value",b);
  else
    printf("%d is the biggest value",c);
  getch();
}  
  
  
